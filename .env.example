# SWING_BOT Environment Variables

# Upstox API Configuration
UPSTOX_API_KEY=your_api_key_here
UPSTOX_API_SECRET=your_api_secret_here
UPSTOX_ACCESS_TOKEN=your_access_token_here

# ICICI Direct Breeze API Configuration
ICICI_API_KEY=your_icici_api_key_here
ICICI_API_SECRET=your_icici_api_secret_here

# Telegram Bot Configuration (for alerts)
TELEGRAM_BOT_TOKEN=your_telegram_bot_token_here
TELEGRAM_CHAT_ID=your_telegram_chat_id_here
ICICI_SESSION_TOKEN=your_icici_session_token_here

# Zerodha/Kite API Configuration (for live trading)
KITE_API_KEY=your_kite_api_key_here
KITE_API_SECRET=your_kite_api_secret_here
KITE_ACCESS_TOKEN=your_kite_access_token_here
KITE_PUBLIC_TOKEN=your_kite_public_token_here

# Teams Notifications
TEAMS_WEBHOOK_URL=https://outlook.office.com/webhook/your_webhook_url_here
TEAMS_ENABLED=true

# Telegram Notifications
TELEGRAM_BOT_TOKEN=your_telegram_bot_token_here
TELEGRAM_CHAT_ID=your_telegram_chat_id_here
TELEGRAM_ENABLED=true

# Email Notifications (SMTP Fallback)
EMAIL_ENABLED=true
EMAIL_PROVIDER=smtp
SMTP_SERVER=smtp.gmail.com
SMTP_PORT=587
SMTP_USERNAME=your_email@gmail.com
SMTP_PASSWORD=your_app_password_here
FROM_EMAIL=your_email@gmail.com
TO_EMAILS=ops@company.com,alerts@company.com

# Microsoft Graph Email (Optional Alternative)
# GRAPH_TENANT_ID=your_tenant_id
# GRAPH_CLIENT_ID=your_client_id
# GRAPH_CLIENT_SECRET=your_client_secret
# GRAPH_MAILBOX_UPN=user@company.com

# Notification Settings
FALLBACK_ENABLED=true

# Metrics and Monitoring
METRICS_ENABLED=true
METRICS_EXPORTER=prometheus
METRICS_PORT=9108

# OpenTelemetry (if using OTLP instead of Prometheus)
# OTLP_ENDPOINT=http://localhost:4317

# Dashboard Settings
DASHBOARD_ENABLED=true

# Logging
LOG_LEVEL=INFO
ENVIRONMENT=production