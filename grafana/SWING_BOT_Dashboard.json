{
  "dashboard": {
    "id": null,
    "title": "SWING_BOT Monitoring Dashboard",
    "tags": ["swingbot", "trading", "monitoring"],
    "timezone": "Asia/Kolkata",
    "panels": [
      {
        "id": 1,
        "title": "Data Quality Metrics",
        "type": "stat",
        "targets": [
          {
            "expr": "swingbot_data_freshness_days",
            "refId": "A"
          },
          {
            "expr": "swingbot_coverage_days",
            "refId": "B"
          },
          {
            "expr": "swingbot_symbols_count",
            "refId": "C"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "mappings": [],
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {
                  "color": "green",
                  "value": null
                },
                {
                  "color": "red",
                  "value": 2
                }
              ]
            }
          }
        },
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 0
        }
      },
      {
        "id": 2,
        "title": "Audit Results",
        "type": "bargauge",
        "targets": [
          {
            "expr": "swingbot_audit_pass_count",
            "refId": "A",
            "legendFormat": "Pass Count"
          },
          {
            "expr": "swingbot_audit_fail_count",
            "refId": "B",
            "legendFormat": "Fail Count"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "mappings": [],
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {
                  "color": "green",
                  "value": null
                }
              ]
            }
          }
        },
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 0
        }
      },
      {
        "id": 3,
        "title": "Orchestration Runtime",
        "type": "histogram",
        "targets": [
          {
            "expr": "rate(swingbot_orchestrate_runtime_seconds_bucket[5m])",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "mappings": [],
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {
                  "color": "green",
                  "value": null
                },
                {
                  "color": "red",
                  "value": 60
                }
              ]
            }
          }
        },
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 8
        }
      },
      {
        "id": 4,
        "title": "Data Fetch Runtime",
        "type": "histogram",
        "targets": [
          {
            "expr": "rate(swingbot_fetch_runtime_seconds_bucket[5m])",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "mappings": [],
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {
                  "color": "green",
                  "value": null
                },
                {
                  "color": "red",
                  "value": 30
                }
              ]
            }
          }
        },
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 8
        }
      },
      {
        "id": 5,
        "title": "Run Frequency",
        "type": "stat",
        "targets": [
          {
            "expr": "rate(swingbot_runs_total[1h]) * 3600",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "mappings": [],
            "unit": "runs/hour",
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {
                  "color": "green",
                  "value": null
                },
                {
                  "color": "yellow",
                  "value": 0.5
                },
                {
                  "color": "red",
                  "value": 2
                }
              ]
            }
          }
        },
        "gridPos": {
          "h": 8,
          "w": 8,
          "x": 0,
          "y": 16
        }
      },
      {
        "id": 6,
        "title": "Notifications Sent",
        "type": "table",
        "targets": [
          {
            "expr": "sum by (channel) (swingbot_notifications_sent_total)",
            "refId": "A",
            "format": "table"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "mappings": []
          }
        },
        "gridPos": {
          "h": 8,
          "w": 8,
          "x": 8,
          "y": 16
        }
      },
      {
        "id": 7,
        "title": "System Health",
        "type": "status-history",
        "targets": [
          {
            "expr": "up{job=\"swingbot\"}",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "mappings": [],
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {
                  "color": "red",
                  "value": null
                },
                {
                  "color": "green",
                  "value": 1
                }
              ]
            }
          }
        },
        "gridPos": {
          "h": 8,
          "w": 8,
          "x": 16,
          "y": 16
        }
      }
    ],
    "time": {
      "from": "now-24h",
      "to": "now"
    },
    "timepicker": {},
    "templating": {
      "list": []
    },
    "annotations": {
      "list": []
    },
    "refresh": "5m",
    "schemaVersion": 36,
    "version": 1,
    "links": []
  }
}